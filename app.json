{
  "expo": {
    "name": "Thermo Track",
    "slug": "asf-expo-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pig.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pig.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "BACKGROUND_FETCH",
        "WAKE_LOCK"
      ],
      "package": "com.shintakino.asfexpov1",
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pig.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pig.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/pig.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/notif1.wav",
            "./assets/sounds/notif2.wav"
          ],
          "androidMode": "default",
          "androidCollapsedTitle": "Thermo Track",
          "androidImportance": "high"
        }
      ],
      "expo-background-task"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4aa1d716-8cb5-46c8-aa48-4e3b5ecf54c1"
      }
    }
  }
}
